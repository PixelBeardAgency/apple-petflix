{
  "name": "petflix-backend",
  "version": "1.0.0",
  "description": "Petflix backend API server",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:unit": "jest --testPathPattern=__tests__ --passWithNoTests",
    "test:security": "jest --testPathPattern=security.test --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "petflix",
    "api",
    "youtube",
    "pets"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "googleapis": "^129.0.0",
    "helmet": "^7.1.0",
    "node-cache": "^5.1.2",
    "web-push": "^3.6.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.3",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
